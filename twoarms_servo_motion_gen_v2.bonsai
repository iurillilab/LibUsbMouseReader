              <ipy:Description>Compute servo motor angles given target coordinates and arms lengths</ipy:Description>
              <ipy:Script>from math import sin, cos, atan2, acos, pi
from System import Tuple

def _rad_to_deg(r):
  return (180*(r/pi))

#@returns(list)  #IList[Tuple[float, float]])
#@returns(Tuple[int,int])
@returns(Tuple[float,float])
def process(values):
  x, y, arm1_length, arm2_length = values
  # Compute angles of the two arms:
  beta = acos((x**2 + y**2 - arm1_length**2 - arm2_length**2) / (2 * arm1_length * arm2_length))
  alpha = atan2(y, x) - atan2(arm2_length * sin(beta), arm1_length + arm2_length * cos(beta))

  #return Tuple.Create(_rad_to_deg(alpha), _rad_to_deg(beta))
  return Tuple.Create(alpha, beta)</ipy:Script>
