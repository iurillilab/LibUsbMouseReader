            <Expression xsi:type="ipy:PythonTransform">
              <ipy:Name>GenerateImage</ipy:Name>
              <ipy:Script>import clr
clr.AddReference("OpenCV.Net")
from OpenCV.Net import *
import OpenCV.Net as n
from math import cos, sin, pi

# Accumulators for keeping history
trajectory = None
prevpoint = None
color = Scalar.Rgb(255,0,0)
thickness = 3
scatter_size = 20

v_offset, h_offset = (200, 500)
scale = 25

def _deg_to_rad(angle):
  return pi*(float(angle)/180)


prev_img = IplImage(Size(1000, 1000), IplDepth.U8, 3) #, IplDepth("U8"), 3)
prev_img.SetZero()

@returns(IplImage)
def process(values):
  global trajectory, prev_img, new_img, counter

  angles, length_arm1, length_arm2 = values.Item1, values.Item2, values.Item3

  alpha, beta, centroid_x, centroid_y = angles
  #alpha = _deg_to_rad(alpha)
  #beta = _deg_to_rad(beta)

  # First joint:
  v_motor2 = (cos(alpha)*length_arm1)*scale + v_offset
  h_motor2 = (sin(alpha)*length_arm1)*scale + h_offset

  # object position:
  v_obj = (cos(alpha+beta)*length_arm2)*scale + v_motor2
  h_obj = (sin(alpha+beta)*length_arm2)*scale + h_motor2

  position_motor1 = Point(h_offset, v_offset)
  position_motor2 = Point(h_motor2, v_motor2)
  position_obj = Point(h_obj, v_obj)

  target_position = Point(centroid_y*scale + h_offset, centroid_x*scale + v_offset)
    
  new_img = IplImage(Size(1000, 1000), IplDepth.U8, 3) #, IplDepth("U8"), 3)
  new_img.SetZero()

  # print(position_motor1, position_motor2)
  CV.Line(new_img, position_motor1, position_motor2, color, thickness)
  CV.Line(new_img, position_motor2, position_obj, color, thickness)

  CV.Circle(new_img,target_position,scatter_size,color,-1)
  
  trasp_alpha = 0.4

  CV.AddWeighted(new_img, trasp_alpha, prev_img, 1 - trasp_alpha, 0, prev_img)

  
  return prev_img</ipy:Script>
